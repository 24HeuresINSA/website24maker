<!DOCTYPE html>
<html>
<head>
    <%- include head.ejs %>
</head>

<body>

<!-- Preloader --> <!--
    <!--<section id="preloader">
        <div class="loader" id="loader">
            <div class="loader-img"></div>
        </div>
    </section> -->
<!-- End Preloader -->

<!-- Sidebar Menu -->
<%- include sidebar.ejs %>
<!-- End Sidebar Menu -->

<!-- Site Wraper -->
<div class="wrapper">

    <script src="../js/plugin/jquery.easing.js" type="text/javascript"></script>
    <script src="../js/jquery-ui.min.js" type="text/javascript"></script>
    <script src="../js/bootstrap.min.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.flexslider.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.fitvids.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.viewportchecker.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.stellar.min.js" type="text/javascript"></script>
    <script src="../js/plugin/wow.min.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.colorbox-min.js" type="text/javascript"></script>
    <script src="../js/plugin/owl.carousel.min.js" type="text/javascript"></script>
    <script src="../js/plugin/isotope.pkgd.min.js" type="text/javascript"></script>
    <script src="../js/plugin/masonry.pkgd.min.js" type="text/javascript"></script>
    <script src="../js/plugin/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.fs.tipper.min.js" type="text/javascript"></script>
    <script src="../js/plugin/mediaelement-and-player.min.js"></script>
    <script src="../js/theme.js" type="text/javascript"></script>
    <script src="../js/plugin/jquery.downCount.js" type="text/javascript"></script>

    <style type="text/css">
        /* counter clock css */
        ul.countdown {
            list-style: none;
            padding: 0;
            display: block;
            text-align: center;
            font-family: 'arial', sans-serif !important;
        }

        .font-billetterie-date3{
            font-size: 50px;
        }

        ul.countdown li {
            display: inline-block;
        }

        ul.countdown li span {
            font-size: 75px;
            font-weight: 300;
            color: #E20917;

        }

        ul.countdown li.seperator {
            font-size: 3vw;
            line-height: 30px;
            vertical-align: top;
            color: #E20917;

        }

        ul.countdown li p {
            color: #E20917;
            font-size: 3vw;
        }
        /* emd counter clock css */

        @media (max-width: 767px) {

            .text-center-xs {
                text-align: center;
            }
        }

        @media (min-width: 1000px) {
            .font-billetterie-date{
                font-size: 90px;
            }

            .font-billetterie-date2{
                font-size: 30px;
            }

            ul.countdown li span {
                font-size: 90px;
                color: #E20917;
                font-weight: 300;
            }

            ul.countdown li.seperator {
                font-size: 60px;
                line-height: 40px;
                vertical-align: top;
                color: #E20917;

            }

            ul.countdown li p {
                color: #E20917;
                font-size: 28px;
            }
        }

        @media (max-width: 600px) {
            .font-billetterie-date{
                font-size: 90px;
            }
            .font-billetterie-date2{
                font-size: 15px;
            }

            ul.countdown li span {
                font-size: 30px;
                color: #E20917;
                font-weight: 300;
            }

            ul.countdown li.seperator {
                font-size: 15px;
                line-height: 40px;
                vertical-align: top;
                color: #E20917;

            }

            ul.countdown li p {
                color: #E20917;
                font-size: 15px;
            }
        }
    </style>

    <!-- Header ("header--dark", "header-transparent", "header--sticky")-->
    <%- include header.ejs %>
    <!-- End Header -->

    <!-- JS -->

    <!-- CONTENT --------------------------------------------------------------------------------->

    <section class="inner-intro dark-bg bg-image overlay-dark parallax parallax-background5"
             style="background-position: center;
                    top: 50px"
             data-background-img="/img/background/webtv.jpg">
        <div class="container m-auto"
             style="display: flex;
                    justify-content: center;
                    flex-direction: column">
            <div class="testimonial text-center pb-lg-4">
                <span class="font-billetterie-date3">
                    Retrouvez la WebTV des 24 Heures de l’INSA
                </span><br>
                <span class="font-billetterie-date2">
                    entre 13h30 et 18h le Samedi 22 Mai et le Dimanche 23 Mai. Vous pourrez profiter de diverses animations prévues par toute l’équipe des 24 Heures de l’INSA !
                </span>
            </div>
            <div class="testimonial text-center dark-color">
                <ul class="countdown pt-4 pb-80 " style="color: #E20917;">
                    <li>
                        <span class="days">00</span>
                        <p class="days_ref">days</p>
                    </li>
                    <li class="seperator">.</li>
                    <li>
                        <span class="hours">00</span>
                        <p class="hours_ref">hours</p>
                    </li>
                    <li class="seperator">:</li>
                    <li>
                        <span class="minutes">00</span>
                        <p class="minutes_ref">minutes</p>
                    </li>
                    <li class="seperator">:</li>
                    <li>
                        <span class="seconds">00</span>
                        <p class="seconds_ref">seconds</p>
                    </li>
                </ul>
                    <div class="live-button d-none">
                        <h3>
                            <a href="https://www.youtube.com"> Rejoindre le live ici </a>
                        </h3>
                    </div>
                </div>
        </div>
    </section>

    <!-- END CONTENT ---------------------------------------------------------------------------->

    <!-- FOOTER -->
    <div style="position: fixed;
                bottom: 0;
                width: 100%;
                z-index: 999">
    <%- include footer.ejs %>
    </div>
    <!-- END FOOTER -->



    <!-- Scroll Top -->
    <a class="scroll-top">
        <i class="fa fa-angle-double-up"></i>
    </a>
    <!-- End Scroll Top -->

</div>
<!-- Site Wraper End -->

<!-- JS -->
<script type="text/javascript">
    $('.countdown').downCount({
        date: '05/21/2021 20:00:00',
        offset: +1
    }, function () {
        return;
    });
    $("#menu-live").css('color', "#E20917");
</script>

</body>
</html>
