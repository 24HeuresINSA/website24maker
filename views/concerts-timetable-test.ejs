<!DOCTYPE html>
<html>
<head>
    <%- include head.ejs %>
    <link href="/css/timetable.css" rel="stylesheet" type="text/css" />
    <script src="/js/timetable.js" type="text/javascript"></script>
</head>

<body>

<!-- Preloader --> <!--
<section id="preloader">
    <div class="loader" id="loader">
        <div class="loader-img"></div>
    </div>
</section>
<!-- End Preloader -->

<!-- Sidebar Menu -->
<%- include sidebar.ejs %>
<!-- End Sidebar Menu -->

<!-- Site Wraper -->
<div class="wrapper">

    <!-- Header ("header--dark", "header-transparent", "header--sticky")-->
    <%- include header.ejs %>
    <!-- End Header -->


    <!-- CONTENT --------------------------------------------------------------------------------->

    <!-- About Section -->
    <section id="about" class="pt pt-sm-60">
        <div class="container text-center">
            <div class="row">
                <div class="col">
                    <div class="col-md-8 offset-md-2"></div>
                    <h2 class="m-0">
                        <span class="color-concerts">TIMETABLE</span><br />
                    </h2>
                    <h4>
                        <span class="text-original-case">La timetable sera disponible quelques jours avant le festival.</span>
                    </h4>
                </div>
            </div>
        </div>
    </section>
    <!-- End About Section -->

    <!-- Work Section -->

    <section class="ptb-30 pt-2 pt-md-4">
        <div class="container">
            <div class="row">

                <div class="cd-schedule loading">
                    <div class="timeline">
                        <ul>
                            <li><span>20:00</span></li>
                            <li><span>20:30</span></li>
                            <li><span>21:00</span></li>
                            <li><span>21:30</span></li>
                            <li><span>22:00</span></li>
                            <li><span>22:30</span></li>
                            <li><span>23:00</span></li>
                            <li><span>23:30</span></li>
                            <li><span>00:00</span></li>
                            <li><span>00:30</span></li>
                            <li><span>01:00</span></li>
                            <li><span>01:30</span></li>
                            <li><span>02:00</span></li>
                            <li><span>02:30</span></li>
                            <li><span>03:00</span></li>
                            <li><span>03:30</span></li>
                            <li><span>04:00</span></li>
                        </ul>
                    </div> <!-- .timeline -->

                    <div class="events">
                        <ul>
                            <li class="events-group">
                                <div class="top-info"><span>Grande Scène</span></div>

                                <ul>
                                    <li class="single-event" data-start="20:30" data-end="21:30" data-content="event-abs-circuit" data-event="event-1">
                                        <a href="#0">
                                            <em class="event-name">Tremplin Rock</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="11:00" data-end="12:30" data-content="event-rowing-workout" data-event="event-2">
                                        <a href="#0">
                                            <em class="event-name">Rowing Workout</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="14:00" data-end="15:15"  data-content="event-yoga-1" data-event="event-3">
                                        <a href="#0">
                                            <em class="event-name">Yoga Level 1</em>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                            <li class="events-group">
                                <div class="top-info"><span>Petite Scène</span></div>

                                <ul>
                                    <li class="single-event" data-start="10:00" data-end="11:00"  data-content="event-rowing-workout" data-event="event-2">
                                        <a href="#0">
                                            <em class="event-name">Rowing Workout</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="11:30" data-end="13:00"  data-content="event-restorative-yoga" data-event="event-4">
                                        <a href="#0">
                                            <em class="event-name">Restorative Yoga</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="13:30" data-end="15:00" data-content="event-abs-circuit" data-event="event-1">
                                        <a href="#0">
                                            <em class="event-name">Abs Circuit</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="15:45" data-end="16:45"  data-content="event-yoga-1" data-event="event-3">
                                        <a href="#0">
                                            <em class="event-name">Yoga Level 1</em>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                            <li class="events-group">
                                <div class="top-info"><span>Scène Root's</span></div>

                                <ul>
                                    <li class="single-event" data-start="09:00" data-end="10:15" data-content="event-restorative-yoga" data-event="event-4">
                                        <a href="#0">
                                            <em class="event-name">Restorative Yoga</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="10:45" data-end="11:45" data-content="event-yoga-1" data-event="event-3">
                                        <a href="#0">
                                            <em class="event-name">Yoga Level 1</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="12:00" data-end="13:45"  data-content="event-rowing-workout" data-event="event-2">
                                        <a href="#0">
                                            <em class="event-name">Rowing Workout</em>
                                        </a>
                                    </li>

                                    <li class="single-event" data-start="13:45" data-end="15:00" data-content="event-yoga-1" data-event="event-3">
                                        <a href="#0">
                                            <em class="event-name">Yoga Level 1</em>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>

                    <div class="event-modal">
                        <header class="header">
                            <div class="content">
                                <span class="event-date"></span>
                                <h3 class="event-name"></h3>
                            </div>

                            <div class="header-bg"></div>
                        </header>

                        <div class="body">
                            <div class="event-info"></div>
                            <div class="body-bg"></div>
                        </div>

                        <a href="#0" class="close">Close</a>
                    </div>

                    <div class="cover-layer"></div>
                </div>

            </div>
            <!-- End Post Bar -->
        </div>
    </section>

<!-- End Work Section -->

<!-- END CONTENT ---------------------------------------------------------------------------->

<!-- FOOTER -->
<%- include footer.ejs %>
<!-- END FOOTER -->

<!-- Scroll Top -->
<a class="scroll-top">
    <i class="fa fa-angle-double-up"></i>
</a>
<!-- End Scroll Top -->

</div>
<!-- Site Wraper End -->


<!-- JS -->

<script src="../js/plugin/jquery.easing.js" type="text/javascript"></script>
<script src="../js/jquery-ui.min.js" type="text/javascript"></script>
<script src="../js/bootstrap.min.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.flexslider.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.fitvids.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.viewportchecker.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.stellar.min.js" type="text/javascript"></script>
<script src="../js/plugin/wow.min.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.colorbox-min.js" type="text/javascript"></script>
<script src="../js/plugin/owl.carousel.min.js" type="text/javascript"></script>
<script src="../js/plugin/isotope.pkgd.min.js" type="text/javascript"></script>
<script src="../js/plugin/masonry.pkgd.min.js" type="text/javascript"></script>
<script src="../js/plugin/imagesloaded.pkgd.min.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.fs.tipper.min.js" type="text/javascript"></script>
<script src="../js/plugin/mediaelement-and-player.min.js"></script>
<script src="../js/theme.js" type="text/javascript"></script>
<script src="../js/plugin/jquery.downCount.js" type="text/javascript"></script>

<script type="text/javascript">
	$('.countdown').downCount({
		date: '05/17/2019 19:00:00',
		offset: +10
	}, function () {
		alert('La 45ème édition est lancé !');
	});
</script>
<script type="text/javascript">
	$("#title-page-nav").append("Timetable");
</script>



</body>
</html>
