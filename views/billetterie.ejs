		$("#sidebar-menu-billetterie").css('color', '#F11286');
    </script>
    <script type="text/javascript">
		$.ajax({
			url: "/billetterie/stats",
			context: document.body,
			complete: function(xhr, status, errorThrown){
				if(xhr.status == 200){
					var ven = JSON.parse(xhr.responseText).vendredi;
					var sam = JSON.parse(xhr.responseText).samedi;
					var deuxs = JSON.parse(xhr.responseText).deuxsoirs;
					$('.ven-jauge-label').text('ATTEINT '+ ven+'%');
					$('.sam-jauge-label').text('ATTEINT '+ sam+'%');
					$('.deuxs-jauge-label').text('ATTEINT '+ deuxs+'%');
					$('.ven-jauge').attr('data-percent', ven+'%');
					$('.sam-jauge').attr('data-percent', sam+'%');
					$('.deuxs-jauge').attr('data-percent', deuxs+'%');
				}else{
					$(".jauge-billetterie").remove();
				}
				skillsProgressBar();
				function skillsProgressBar() {
					$('.skillbar').each(function () {
						$(this).find('.skillbar-bar').animate({
							width: $(this).attr('data-percent')
						}, 2000);
					});
				};
			}
		});
    </script>

